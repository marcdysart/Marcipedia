



  <div>
    <table class="table table-hover table-bordered collaborator" align="center">
      <h1 class = "text-center">Collaborators</h1>
      <h2 class = "text-center">Choose users who have permission to edit this private wiki.</h2>

      <thead>
        <tr>
          <th >Users</th>
          <th >Collaborator</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user|  %>
        <%= content_tag :tr, class: 'media', id: "colaborator" do %>

        <td>  <%=  user.name %> </td>
        <td>  select  </td>
        <% end %>
        <% end %>
      </tbody>
    </table>
  </div>
